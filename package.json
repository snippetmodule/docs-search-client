{
  "name": "docs-search-client",
  "version": "0.1.0",
  "description": "docs-search-client",
  "scripts": {
    "clear": "rm -rf build/* node_modules/* typings/*",
    "stop": "$(lsof -i :8080 | awk '{print \"kill -9 \" $2}')",
    "postinstall": "typings install",
    "prebuild": "rm -rf build/*",
    "build": "webpack --config config/webpack/index.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "prestart": "npm run build",
    "start": "webpack-dev-server --config config/webpack/index.js --content-base ./build --history-api-fallback --no-info --open",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "lint": "node_modules/.bin/tslint \"src/**/**.ts*\""
  },
  "author": "wfx",
  "homepage": "https://github.com/snippetmodule/docs-search-client.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/snippetmodule/docs-search-client.git"
  },
  "bugs": {
    "url": "https://github.com/snippetmodule/docs-search-client/issues"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "webpack",
    "universal",
    "isomorphic",
    "starter",
    "kit",
    "boilerplate",
    "hot reload",
    "react-hot-loader"
  ],
  "license": "MIT",
  "devDependencies": {
    "chalk": "^1.1.3",
    "classnames": "^2.2.4",
    "compression": "^1.6.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-style-loader": "^1.0.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-assets": "^4.1.0",
    "postcss-cssnext": "^2.6.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.6.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.0.3",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "tslint-react": "^0.4.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "exports-loader": "^0.6.3",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.4.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^3.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-connect": "^2.4.0",
    "redux-thunk": "^2.0.1",
    "trackjs": "^2.4.0"
  }
}